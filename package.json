    "@tanstack/react-query": "^5.90.13",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@types/pg": "^8.16.0",
    "ai": "^5.0.108",
    "better-auth": "^1.4.9",
