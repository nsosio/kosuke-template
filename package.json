{
  "name": "kosuke-template",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:email": "concurrently \"bun run dev\" \"bun run email:dev\"",
    "dev:all": "concurrently \"bun run dev\" \"bun run workers:dev\"",
    "workers:dev": "bun tsx --watch -r dotenv/config worker.ts",
    "workers:start": "bun tsx -r dotenv/config worker.ts",
    "prebuild": "bun run db:migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run lib/db/scripts/db-seed.ts",
    "db:reset": "bun run lib/db/scripts/db-reset.ts && bun run db:migrate && bun run db:seed",
    "db:create-admin-user": "bun run lib/db/scripts/db-create-admin-user.ts",
    "email:dev": "email dev --dir emails --port 3001",
    "email:export": "email export --dir emails --out .react-email",
    "shadcn:update": "bun shadcn@canary add -a -y -o",
    "shadcn:check": "bun shadcn@canary diff",
    "knip": "knip"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.47",
    "@ai-sdk/react": "^2.0.115",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/genai": "^1.34.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
